//js code
// all arthemtic operation
let a=8;
let b=2;
console.log(a+b);
console.log(a-b);
console.log(a*b);
console.log(a/b);
console.log(a%b);
console.log(a**b);
a++;
console.log(a);
++a;
console.log(a);
b--;
console.log(b);
--b;
console.log(b);
